{
  "lockFileMaintenance": {
    "enabled": true
  },
  "poetry": {
    "enabled": true
  },
  "pip_setup": {
    "enabled": false
  },
  "pip_requirements": {
    "enabled": false
  },
  "pipenv": {
    "enabled": false
  },
  "setup-cfg": {
    "enabled": false
  },
  "packageRules": [
    {
      "matchManagers": [
        "poetry"
      ],
      "semanticCommitType": "build",
      "groupName": "deps",
      "matchDepTypes": [
        "dependencies"
      ],
      "semanticCommitScope": "deps"
    },
    {
      "matchManagers": [
        "poetry"
      ],
      "groupName": "flake8",
      "semanticCommitType": "build",
      "matchDepTypes": [
        "dependencies"
      ],
      "matchPackagePrefixes": [
        "flake8"
      ]
    },
    {
      "matchManagers": [
        "poetry"
      ],
      "groupName": "sphinx",
      "semanticCommitType": "build",
      "matchDepTypes": [
        "dependencies"
      ],
      "matchPackagePrefixes": [
        "sphinx"
      ]
    },
    {
      "matchManagers": [
        "poetry"
      ],
      "semanticCommitType": "chore",
      "groupName": "deps-dev",
      "matchDepTypes": [
        "dev-dependencies"
      ],
      "semanticCommitScope": "dev"
    }
  ]
}
