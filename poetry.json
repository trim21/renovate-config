{
  "lockFileMaintenance": {
    "semanticCommitType": "build",
    "enabled": true
  },
  "poetry": {
    "enabled": true
  },
  "pip_setup": {
    "enabled": false
  },
  "pip_requirements": {
    "enabled": false
  },
  "pipenv": {
    "enabled": false
  },
  "setup-cfg": {
    "enabled": false
  },
  "packageRules": [
    {
      "groupName": "deps",
      "matchManagers": [
        "poetry"
      ],
      "semanticCommitType": "build",
      "matchDepTypes": [
        "dependencies"
      ],
      "matchUpdateTypes": [
        "minor",
        "patch",
        "digest",
        "pin",
        "pinDigest"
      ],
      "semanticCommitScope": "deps"
    },
    {
      "groupName": "flake8",
      "matchManagers": [
        "poetry"
      ],
      "semanticCommitType": "style",
      "matchUpdateTypes": [
        "minor",
        "patch",
        "digest",
        "pin",
        "pinDigest"
      ],
      "matchPackagePrefixes": [
        "flake8"
      ]
    },
    {
      "groupName": "sphinx",
      "matchManagers": [
        "poetry"
      ],
      "semanticCommitType": "build",
      "matchUpdateTypes": [
        "minor",
        "patch",
        "digest",
        "pin",
        "pinDigest"
      ],
      "matchPackagePrefixes": [
        "sphinx"
      ]
    },
    {
      "groupName": "deps-dev",
      "matchManagers": [
        "poetry"
      ],
      "semanticCommitType": "chore",
      "matchDepTypes": [
        "dev-dependencies"
      ],
      "matchUpdateTypes": [
        "minor",
        "patch",
        "digest",
        "pin",
        "pinDigest"
      ],
      "semanticCommitScope": "dev",
      "rangeStrategy": "pin"
    }
  ]
}
