{
  "enabledManagers": [
    "npm"
  ],
  "lockFileMaintenance": {
    "extends": [
      "@trim21:monthly"
    ],
    "enabled": true
  },
  "packageRules": [
    {
      "matchPackageNames": "^nodejs/node$",
      "matchDepTypes": [
        "engines"
      ],
      "enabled": false
    },
    {
      "groupName": "deps",
      "matchDepTypes": [
        "dependencies",
        "devDependencies"
      ]
    }
  ]
}
