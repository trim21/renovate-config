{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "pyenv":{
    "enabled": false
  },
  "packageRules": [
    {
      "matchManagers": [
        "dockerfile"
      ],
      "matchPackageNames": [
        "python"
      ],
      "matchUpdateTypes": [
        "major",
        "minor",
        "bump",
        "replacement"
      ],
      "enabled": false
    }
  ]
}
